{
    "name": "agenter-labs/illuminate-version",
    "description": "Version management package for Laravel",
    "keywords": [
        "laravel",
        "version"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Abdulla",
            "email": "dev@agenter.com",
            "homepage": "https://agenter.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "AgenterLab\\Version\\": "./src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "AgenterLab\\Version\\Provider"
            ],
            "aliases": {
                "Version": "AgenterLab\\Version\\Facade"
            }
        }
    }
}
